FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 34.4993;

boundaryField
{
    bottom
    {
        type            epsilonWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 34.4993;
    }
    top
    {
        type            epsilonWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 34.4993;
    }
    west
    {
        type            fixedValue;
        value           uniform 34.4993;
    }
    east
    {
        type            zeroGradient;
    }
    north
    {
        type            zeroGradient;
    }
    south
    {
        type            fixedValue;
        value           uniform 34.4993;
    }
}


